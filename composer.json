{
  "name": "denismitr/denismitr",
  "description": "Easy to use and mockable HTTP client, wraps most common http calls functionality around Guzzle.",
  "keywords": ["http", "guzzle", "request", "client"],
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Mitrofanov",
      "email": "denis.mitr@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.0",
    "tightenco/collect": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "laravel/lumen-framework": "5.5.*",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Denismitr\\NetCall\\": "src/"
    },
    "files": [
      "src/helpers/tap.php"
    ]
  },
  "scripts": {
    "test": "php vendor/phpunit/phpunit/phpunit"
  }
}
